{
  "name": "wrightstream",
  "version": "0.1.0",
  "description": "The ultimate workflow tool for artisans",
  "private": true,
  "main": "./public/index.html",
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/_sass/main.scss src/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/css/main.css src/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "start": "npm run css-watch | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "html",
    "css",
    "sass",
    "javascript",
    "jquery",
    "react",
    "redux",
    "bulma",
    "axios"
  ],
  "author": [
    "Claudia Ligidakis <cligidakis@gmail.com>",
    "Vikanda Gonzales <vikanda.gonzales@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@vizuaalog/bulmajs": "^0.3.1",
    "autoprefixer": "^8.5.0",
    "axios": "^0.18.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^1.0.30",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "reactbulma": "^3.0.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0"
  }
}
