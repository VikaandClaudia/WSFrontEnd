{
  "name": "wrightstream",
  "version": "0.1.0",
  "description": "The ultimate workflow tool for artisans",
  "private": true,
  "main": "./public/index.html",
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "node-sass src/_sass/main.scss src/css/main.css",
    "css-postcss": "postcss --use autoprefixer --output src/css/main.css src/css/main.css",
    "css-watch": "npm run css-build -- --watch",
    "js-build": "babel src/_javascript --out-dir src/lib",
    "js-watch": "npm run js-build -- --watch",
    "start": "npm run css-watch | npm run js-watch | react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "keywords": [
    "html",
    "css",
    "sass",
    "javascript",
    "jquery",
    "react",
    "redux",
    "bulma",
    "axios"
  ],
  "author": [
    "Claudia Ligidakis <cligidakis@gmail.com>",
    "Vikanda Gonzales <vikanda.gonzales@gmail.com>"
  ],
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/react-fontawesome": "0.0.20",
    "@vizuaalog/bulmajs": "^0.3.1",
    "autoprefixer": "^8.5.0",
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015-ie": "^6.7.0",
    "bulma": "^0.7.1",
    "bulma-extensions": "^1.0.30",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.0",
    "qs": "^6.5.2",
    "react": "^16.3.2",
    "react-beautiful-dnd": "^7.1.3",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-form": "^7.3.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "shortid": "^2.2.8"
  }
}
